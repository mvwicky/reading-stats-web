{
  "typescript.tsdk": "node_modules/typescript/lib",
  "files.exclude": {
    "**/.git": true,
    "**/.svn": true,
    "**/.hg": true,
    "**/CVS": true,
    "**/.DS_Store": true,
    "**/Thumbs.db": true,
    "**/node_modules": true
  },
  "editor.codeActionsOnSave": { "source.fixAll": false },
  "[svelte]": { "editor.codeActionsOnSave": { "source.fixAll": true } },
  "[typescript]": { "editor.codeActionsOnSave": { "source.fixAll": true } }
}
